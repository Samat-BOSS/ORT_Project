@model ORT_Project.Models.School

@{
    ViewBag.Title = "Учебные учреждения - удаление заиси";
}

<h2>Удалить запись</h2>

<h3>Вы уверены что хотите удалить эту запись?</h3>
<div>
    <h4>данные учебного учреждения</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @*@Html.DisplayNameFor(model => model.School_name)*@
            Учебное учреждение
        </dt>

        <dd>
            @Html.DisplayFor(model => model.School_name)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.Postal_zip)*@
            Почтовый код
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Postal_zip)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.Code_of_school)*@
            Код школы
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Code_of_school)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.Password_school)*@
            Пароль школы
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Password_school)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.LocalitiName)*@
            Населенный пункт
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LocalitiName)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.LocalityAddress)*@
            Адрес
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LocalityAddress)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.District_town_.District_name)*@
            Район
        </dt>

        <dd>
            @Html.DisplayFor(model => model.District_town_.District_name)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Удалить запись" class="btn btn-default" /> |
            @Html.ActionLink("Назад", "Index")
        </div>
    }
</div>
