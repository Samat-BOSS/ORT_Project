@model IEnumerable<ORT_Project.Models.Limit_of_time>

@{
    ViewBag.Title = "Ограничения по времени";
}

<h2>Ограничения по времени</h2>

<p>
    @Html.ActionLink("Создать запись", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.Date_of_limit)*@
            Дата установки лимита
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Limit_start)*@
            Начала периода
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Limit_end)*@
            Конец периода
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date_of_limit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Limit_start)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Limit_end)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.ID_Time }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.ID_Time })
        </td>
    </tr>
}

</table>
