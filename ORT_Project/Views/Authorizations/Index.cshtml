@model IEnumerable<ORT_Project.Models.Authorization>

@{
    ViewBag.Title = "Данные авторизации";
}

<h2>Данные авторизации</h2>

<p>
    @Html.ActionLink("Добавить запись", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.Login)*@
            Логин
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Password)*@
            Пароль
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.id_Autorization }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.id_Autorization })
        </td>
    </tr>
}

</table>
